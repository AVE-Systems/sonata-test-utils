{
  "name": "ave-systems/sonata-test-utils",
  "description": "Utils for testing SonataAdminBundle",
  "homepage": "https://github.com/AVE-Systems/sonata-test-utils",
  "type": "library",
  "minimum-stability": "stable",
  "keywords": [
    "trait", "php7", "helper"
  ],
  "require": {
    "php": "^7.2",
    "ext-json": "*",
    "symfony/dom-crawler": "^3.1",
    "symfony/css-selector": "^2.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.3"
  },
  "autoload": {
    "psr-4": {
      "AveSystems\\SonataTestUtils\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}